                         "LITI{1}=<strong><em>@w{\1}:</em></strong>" \
                         "IMG{2}=\image html \1 \"\2\"" \
                         "TR{1}=<tr>@w{\1}</tr>" \
                         "TD{1}=<td>@w{\1}</td>" \
